{
  "name": "YOUR PROJECT NAME",
  "description": "",
  "version": "1.0.0",
  "author": "",
  "license": "",
  "repository": {
    "type": "git",
    "url": "URL TO YOUR REPOSITORY"
  },
  "bugs": {
    "url": "URL TO ISSUE TRACKER"
  },
  "scripts": {
    "clean": "rimraf DIRECTORY TO CLEAN OUT",
    "start": "pm2-runtime start ecosystem.config.js --env production",
    "start:prettier:dev": "npm-run-all --parallel open:src:dev prettier-watch",
    "clean:build:dev": "run-s clean build:dev open:src:dev",
    "open:src:dev": "nodemon bin/www",
    "prettier-watch": "onchange '**/*.js' -- prettier --write {{changed}}",
    "build:dev": "webpack --config webpack.dev.config.js",
    "build:prod": "webpack --config webpack.prod.config.js"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.5",
    "debug": "~4.3.1",
    "dotenv": "^8.2.0",
    "ejs": "~3.1.6",
    "express": "~4.17.1",
    "express-sslify": "^1.2.0",
    "http-errors": "~1.8.0",
    "morgan": "~1.10.0",
    "pm2": "^4.5.5",
    "regenerator-runtime": "^0.13.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.8",
    "@babel/node": "^7.13.0",
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-env": "^7.13.9",
    "@babel/register": "^7.13.8",
    "ajv": "^7.1.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-airbnb": "^5.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.1.1",
    "eslint": "^7.21.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "husky": "^5.1.3",
    "lint-staged": "^10.5.4",
    "mini-css-extract-plugin": "^1.3.9",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.24.3",
    "webpack-cli": "^4.5.0",
    "webpack-dev-middleware": "^4.1.0",
    "webpack-manifest-plugin": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write"
    ],
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "babel": {
    "presets": [
      [
        "airbnb",
        {
          "targets": {
            "node": "12.18.0"
          }
        },
        "@babel/preset-env"
      ]
    ],
    "plugins": [
      "babel-plugin-transform-class-properties"
    ]
  }
}
